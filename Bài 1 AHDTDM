#include <stdio.h>

int isMultipleOf7(int number) {
    return number % 7 == 0;
}

void printTwoDigitMultiplesOf7() {
    int i;
    for (i = 10; i < 100; i++) {
        if (isMultipleOf7(i)) {
            printf("%d ", i);
        }
    }
}

int main() {
    printf("Cac so nguyen co 2 chu so va la boi cua 7 la:\n");
    printTwoDigitMultiplesOf7();
    return 0;
}
